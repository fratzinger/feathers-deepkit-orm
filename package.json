{
  "name": "feathers-deepkit-orm",
  "description": "A feathers.js service adapter for @deepkit/orm",
  "version": "0.0.1",
  "homepage": "https://github.com/fratzinger/feathers-deepkit-orm",
  "keywords": [
    "feathers",
    "feathers-plugin",
    "deepkit",
    "orm",
    "mysql",
    "sqlite",
    "mariadb",
    "postgres",
    "pg",
    "mssql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fratzinger/feathers-deepkit-orm.git"
  },
  "author": {
    "name": "Feathers contributors",
    "email": "hello@feathersjs.com",
    "url": "https://feathersjs.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/fratzinger/feathers-deepkit-orm/issues"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "build": "shx rm -rf dist/ && npm run tsc",
    "tsc": "tsc -p tsconfig.json",
    "publish": "git push origin --tags && npm run changelog && git push origin",
    "changelog": "github_changelog_generator -u fratzinger -p feathers-deepkit-orm && git add CHANGELOG.md && git commit -am \"Updating changelog\"",
    "update-dependencies": "ncu -u",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "mocha": "cross-env NODE_ENV=test TS_NODE_PROJECT='tsconfig.test.json' mocha --timeout 10000",
    "test": "npm run lint && npm run coverage",
    "coverage": "nyc npm run mocha"
  },
  "dependencies": {
    "@deepkit/core": "^1.0.1-alpha.59",
    "@deepkit/injector": "^1.0.1-alpha.59",
    "@deepkit/logger": "^1.0.1-alpha.59",
    "@deepkit/orm": "^1.0.1-alpha.59",
    "@deepkit/stopwatch": "^1.0.1-alpha.59",
    "@feathersjs/adapter-commons": "^4.5.12",
    "@feathersjs/commons": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2"
  },
  "devDependencies": {
    "@deepkit/sqlite": "^1.0.1-alpha.59",
    "@deepkit/type": "^1.0.1-alpha.59",
    "@feathersjs/adapter-tests": "^4.5.12",
    "@feathersjs/express": "^4.5.12",
    "@feathersjs/feathers": "^4.5.12",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "body-parser": "^1.19.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "mocha": "^9.2.0",
    "npm-check-updates": "^12.2.1",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
}
